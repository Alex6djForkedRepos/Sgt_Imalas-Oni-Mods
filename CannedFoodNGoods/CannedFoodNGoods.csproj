	<!-- Build Properties -->
	<PropertyGroup>
		<TargetFramework>net471</TargetFramework>
		<Configurations>Debug</Configurations>
		<AssemblyName>CannedFoods</AssemblyName>
		<RootNamespace>CannedFoods</RootNamespace>
		<IsMod>true</IsMod>
		<GenerateMetadata>true</GenerateMetadata>
		<IsPacked>true</IsPacked>
	</PropertyGroup>


	<!-- Mod Info -->
	<PropertyGroup>
		<ModName>Canned Food</ModName>
		<ModDescription>Adds canned meat and fish.</ModDescription>
		<SupportedContent>ALL</SupportedContent>
		<MinimumSupportedBuild>509629</MinimumSupportedBuild>
		<APIVersion>2</APIVersion>
	</PropertyGroup>



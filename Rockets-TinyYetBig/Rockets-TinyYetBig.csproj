		<Authors>Sgt_Imalas</Authors>
		<Copyright>2022 Sgt_Imalas</Copyright>
		<RepositoryUrl>https://github.com/Knastoron/Knastoron-Oni-Mods</RepositoryUrl>
	</PropertyGroup>

	<!-- Build Properties -->
	<PropertyGroup>
		<TargetFramework>net471</TargetFramework>
		<Configurations>Debug</Configurations>
		<AssemblyName>Rocketry Expanded</AssemblyName>
		<RootNamespace>Rockets_TinyYetBig</RootNamespace>
		<IsMod>true</IsMod>
		<GenerateMetadata>true</GenerateMetadata>
		<IsPacked>true</IsPacked>
	</PropertyGroup>


	<!-- Mod Info -->
	<PropertyGroup>
		<ModName>Rocketry Expanded</ModName>
		<ModDescription>Various QoL Additions to Rockets. Shrinks the border around rocket interiors and provides some larger variants to construct. Also adds several new modules and mechanics to rockets</ModDescription>
		<SupportedContent>EXPANSION1_ID</SupportedContent>
		<MinimumSupportedBuild>525812</MinimumSupportedBuild>
		<APIVersion>2</APIVersion>
	</PropertyGroup>
	<ItemGroup>
	  <Compile Remove="ModAssets\translations\**" />
	  <EmbeddedResource Remove="ModAssets\translations\**" />
	  <None Remove="ModAssets\translations\**" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="PLib" Version="4.8.0">
			<Private>false</Private>
		</PackageReference>
	</ItemGroup>


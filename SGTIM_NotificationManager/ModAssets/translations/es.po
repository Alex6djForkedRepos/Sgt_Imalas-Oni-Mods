msgctxt "SGTIM_NotificationManager.STRINGS.NOTIFICATION_CONFIG.SUFFOCATION.THRESHOLD"
msgid "Suffocation Time Threshold"
msgstr "Umbral de tiempo de asfixia"

#. SGTIM_NotificationManager.STRINGS.NOTIFICATION_CONFIG.SUFFOCATION.THRESHOLD_TOOLTIP
msgctxt "SGTIM_NotificationManager.STRINGS.NOTIFICATION_CONFIG.SUFFOCATION.THRESHOLD_TOOLTIP"
msgid ""
"Time from notification start to death by suffocation in seconds.\n"
"Default is 50 seconds, higher values warn earlier"
msgstr ""
"Tiempo desde la notificación hasta la muerte por asfixia en segundos.\n"
"Por defecto: 50 segundos, valores mayores avisan antes"

#. SGTIM_NotificationManager.STRINGS.NOTIFICATION_CONFIG.ZOOM_ON_NOTIFICATION
msgctxt "SGTIM_NotificationManager.STRINGS.NOTIFICATION_CONFIG.ZOOM_ON_NOTIFICATION"
